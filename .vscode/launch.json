{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug Real Image Classification Model",
      "program": "${workspaceFolder}/target/debug/examples/real_model_example",
      "args": [],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "stopOnEntry": false,
      "env": {
        "DYLD_LIBRARY_PATH": "${workspaceFolder}/target/debug",
        "RUST_BACKTRACE": "1"
      }
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug Real Object Detection Model",
      "program": "${workspaceFolder}/target/debug/examples/object_detection_example",
      "args": [],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "stopOnEntry": false,
      "env": {
        "DYLD_LIBRARY_PATH": "${workspaceFolder}/target/debug",
        "RUST_BACKTRACE": "1"
      },
      "preLaunchTask": "rust: cargo build --example object_detection_example"
    }
  ]
}